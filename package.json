{
  "name": "johan_martin_personal_site",
  "version": "0.0.8",
  "description": "Johan Martin Site",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "npm-run-all -n -p build:gulp prod:server",
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 npm-run-all -n -p gulp:watch dev:server lint:watch",
    "dev:dashboard": "webpack-dashboard -- npm-run-all -n -p gulp:watch dev:server lint:watch",
    "dev:watch": "webpack -w -d",
    "prod:server": "cross-env NODE_ENV=production webpack-dev-server --color --progress",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --color --progress",
    "gulp:clean": "gulp build",
    "build": "npm-run-all -s build:gulp build:webpack",
    "dev:build": "cross-env NODE_ENV=development npm-run-all -s build:gulp webpack:dev",
    "webpack:dev": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "gulp:watch": "gulp -LLL --color",
    "build:gulp": "gulp build",
    "build:style": "gulp style",
    "gh-deploy:build": "npm-run-all -n -s dev:build gh-deploy",
    "gh-deploy": "gh-pages -d dist",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catenare/personal-site"
  },
  "keywords": [
    "Static",
    "webpack",
    "theme"
  ],
  "author": "Johan Martin <martin.johan@johan-martin.com> (http://www.johan-martin.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catenare/personal-site/issues"
  },
  "homepage": "https://github.com/catenare/personal-site",
  "dependencies": {
    "axios": "^0.18.0",
    "font-awesome": "^4.7.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/uuid": "^3.4.3",
    "autoprefixer": "^8.6.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^3.1.5",
    "expect": "^23.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flag-icon-css": "^3.0.0",
    "foundation-sites": "^6.4.4-rc1",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^2.0.4",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "panini": "^1.6.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-scss": "^1.0.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "style-sherpa": "^1.0.2",
    "stylelint": "^8.4.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.5.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "max-nesting-depth": 3,
      "order/properties-alphabetical-order": false
    }
  },
  "browserslist": [
    "last 2 versions",
    "safari >= 7",
    ">1%"
  ]
}
