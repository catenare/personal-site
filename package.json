{
  "name": "multi_page_template",
  "version": "0.0.1",
  "description": "Basic Multipage Template",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "npm-run-all -n -p build:gulp prod:server",
    "dev": "npm-run-all -n -p gulp:watch dev:server lint:watch",
    "dev:dashboard": "webpack-dashboard -- npm-run-all -n -p gulp:watch dev:server lint:watch",
    "dev:watch": "webpack -w -d",
    "prod:server": "cross-env NODE_ENV=production webpack-dev-server --color --progress",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --color --progress",
    "gulp:clean": "gulp build",
    "build": "npm-run-all -s build:gulp build:webpack",
    "dev:build": "cross-env NODE_ENV=development npm-run-all -s build:gulp webpack:dev",
    "webpack:dev": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "gulp:watch": "gulp -LLL --color",
    "build:gulp": "gulp build",
    "build:style": "gulp style",
    "gh-deploy:build": "npm-run-all -n -s dev:build gh-deploy",
    "gh-deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:catenare/webpack-multipage-template.git"
  },
  "keywords": [
    "Static",
    "webpack",
    "theme"
  ],
  "author": "Johan Martin <martin.johan@johan-martin.com> (http://www.johan-martin.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catenare/webpack-multipage-template/issues"
  },
  "homepage": "https://github.com/catenare/webpack-multipage-template",
  "dependencies": {
    "axios": "^0.17.0",
    "csspin": "^1.1.4",
    "jquery": "^3.2.1",
    "motion-ui": "^1.2.3",
    "webfontloader": "^1.6.28",
    "what-input": "^5.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "^3.3.0",
    "babel-core": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.4.4-rc1",
    "gh-pages": "^1.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "html-webpack-plugin": "^2.30.1",
    "js-yaml": "^3.10.0",
    "node-sass": "^4.6.0",
    "npm-run-all": "^4.1.2",
    "panini": "^1.5.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "postcss-scss": "^1.0.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "style-sherpa": "^1.0.2",
    "stylelint": "^8.2.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "tslint": "^5.7.0",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "vinyl-named": "^1.1.0",
    "webpack": "^3.8.1",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^2.9.4",
    "yargs": "^10.0.3"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "max-nesting-depth": 3,
      "order/properties-alphabetical-order": false
    }
  }
}
